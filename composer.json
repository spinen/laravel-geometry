{
    "name": "spinen/laravel-geometry",
    "description": "Wrapper over the geoPHP Class to make it integrate with Laravel better.",
    "keywords": [
        "geometry",
        "geoPHP",
        "laravel",
        "library",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Thai-Son Le",
            "email": "thaison.le@spinen.com"
        },
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "phayes/geophp": "~1.2"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.1",
        "phpunit/phpunit": "~4.0|~5.0",
        "psy/psysh": "^0.5.1",
        "satooshi/php-coveralls": "^0.6.1|^1",
        "symfony/var-dumper": "~2.7|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\Geometry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\Geometry\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\Geometry\\GeometryServiceProvider"
            ],
            "aliases": {
                "Geo": "Spinen\\Geometry\\GeometryFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
