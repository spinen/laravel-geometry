{
    "name": "spinen/laravel-geometry",
    "description": "Wrapper over the geoPHP Class to make it integrate with Laravel better.",
    "keywords": [
        "geometry",
        "geoPHP",
        "laravel",
        "library",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Thai-Son Le",
            "email": "thaison.le@spinen.com"
        },
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "~8|~9|~10|~11",
        "illuminate/support": "~8|~9|~10|~11",
        "phayes/geophp": "~1.2"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^9.6.5",
        "psy/psysh": "^0.11.1",
        "symfony/var-dumper": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\Geometry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\Geometry\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\Geometry\\GeometryServiceProvider"
            ],
            "aliases": {
                "Geo": "Spinen\\Geometry\\GeometryFacade"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
